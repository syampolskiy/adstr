                                                    input#channel_228705.banner(type='checkbox', value='channel 5')
                                                    | channel 5
                                                li
                                                    input#channel_228706.banner(type='checkbox', value='channel 6')
                                                    | channel 6
                            .income-chart
                                canvas#adv-bunners.chart-cnvs.show
                    #tabs-4
                        .streamer-incoms-block
                            .filters-block
                                .datapicker-block
                                    input#startDayView.start-day(name='startDate', readonly='')
                                    input#endDayView.end-day(name='endDate', readonly='')
                                    #date_range_view
                                dl.dropdown.view-dropdown
                                    dt
                                        a(href='#')
                                            span.hida Select
                                            p.view-sel
                                    dd
                                        .views-select
                                            ul
                                                li
